{
    "name": "Calendso",
    "id": "calendso",
    "packaging_format": 1,
    "description": {
        "en": "open-source Calendly alternative",
        "fr": "open-source Calendly alternative"
    },
    "version": "1.0~ynh1",
    "url": "https://calendso.com/",
    "upstream": {
        "license": "MIT",
        "website": "https://calendso.com/",
        "demo": "https://demo.example.com",
        "admindoc": "https://docs.calendso.com/docs/installation",
        "userdoc": "https://yunohost.org/apps",
        "code": "https://github.com/calendso/calendso"
    },
    "license": "MIT",
    "maintainer": {
        "name": "John doe",
        "email": "john.doe@example.com"
    },
    "requirements": {
        "yunohost": ">= 4.2.4"
    },
    "multi_instance": true,
    "services": [
        "nginx",
        "mysql"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "example": "example.com"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/example",
                "default": "/example"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
